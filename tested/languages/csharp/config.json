{
  "general": {
    "dependencies": ["dotnet.csproj", "Values.cs", "EvaluationResult.cs"],
    "selector": true
  },
  "extensions": {
    "file": "cs"
  },
  "naming_conventions": {
    "namespace": "pascal_case",
    "function": "pascal_case",
    "identifier": "pascal_case",
    "property": "pascal_case",
    "class": "pascal_case",
    "global_identifier": "macro_case"
  },
  "constructs": {
    "objects": true,
    "exceptions": true,
    "function_calls": true,
    "assignments": true,
    "heterogeneous_collections": true,
    "heterogeneous_arguments": true,
    "evaluation": true,
    "named_arguments": true,
    "default_parameters": true,
    "global_variables": true
  },
  "datatypes": {
    "integer": "supported",
    "real": "supported",
    "char": "reduced",
    "text": "supported",
    "boolean": "supported",
    "sequence": "supported",
    "set": "supported",
    "map": "supported",
    "nothing": "supported",
    "int8": "supported",
    "uint8": "supported",
    "int16": "supported",
    "uint16": "supported",
    "int32": "supported",
    "uint32": "supported",
    "int64": "supported",
    "uint64": "supported",
    "bigint": "supported",
    "single_precision": "supported",
    "double_precision": "supported",
    "double_extended": "unsupported",
    "fixed_precision": "unsupported",
    "array": "supported",
    "list": "supported",
    "tuple": "supported"
  },
  "restrictions": {
    "map_key": [],
    "set": []
  }
}
